        <action
            android:id="@+id/action_portraitYourPlaylists_to_portraitHome"
            app:destination="@id/portraitHome" />
        <action
            android:id="@+id/action_portraitYourPlaylists_to_portraitAbout"
            app:destination="@id/portraitAbout" />
        <action
            android:id="@+id/action_portraitYourPlaylists_to_portraitYourAlbums"
            app:destination="@id/portraitYourAlbums" />
        <action
            android:id="@+id/action_portraitYourPlaylists_to_portraitTags"
            app:destination="@id/portraitTags" />
    </fragment>
    <fragment
        android:id="@+id/portraitYourAlbums"
        android:name="com.example.mediavault.PortraitYourAlbums"
        android:label="fragment_portrait_your_albums"
        tools:layout="@layout/fragment_portrait_your_albums" >
        <action
            android:id="@+id/action_portraitYourAlbums_to_portraitHome"
            app:destination="@id/portraitHome" />
        <action
            android:id="@+id/action_portraitYourAlbums_to_portraitAbout"
            app:destination="@id/portraitAbout" />
        <action
            android:id="@+id/action_portraitYourAlbums_to_portraitYourPlaylists"
            app:destination="@id/portraitYourPlaylists" />
        <action
            android:id="@+id/action_portraitYourAlbums_to_portraitTags"
            app:destination="@id/portraitTags" />
    </fragment>
    <fragment
        android:id="@+id/portraitTags"
        android:name="com.example.mediavault.PortraitTags"
        android:label="fragment_portrait_tags"
        tools:layout="@layout/fragment_portrait_tags" >
        <action
            android:id="@+id/action_portraitTags_to_portraitHome"
            app:destination="@id/portraitHome" />
        <action
            android:id="@+id/action_portraitTags_to_portraitAbout"
            app:destination="@id/portraitAbout" />
        <action
            android:id="@+id/action_portraitTags_to_portraitYourPlaylists"
            app:destination="@id/portraitYourPlaylists" />
        <action
            android:id="@+id/action_portraitTags_to_portraitYourAlbums"
            app:destination="@id/portraitYourAlbums" />
