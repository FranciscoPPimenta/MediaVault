<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/portraitRegister">

    <fragment
        android:id="@+id/portraitRegister"
        android:name="com.example.mediavault.PortraitRegister"
        android:label="fragment_portrait_register"
        tools:layout="@layout/fragment_portrait_register" >
        <action
            android:id="@+id/action_portraitRegister_to_portraitLogin"
            app:destination="@id/portraitLogin" />
        <action
            android:id="@+id/action_portraitRegister_to_portraitHome"
            app:destination="@id/portraitHome" />
    </fragment>
    <fragment
        android:id="@+id/portraitLogin"
        android:name="com.example.mediavault.PortraitLogin"
        android:label="fragment_portrait_login"
        tools:layout="@layout/fragment_portrait_login" >
        <action
            android:id="@+id/action_portraitLogin_to_portraitRegister"
            app:destination="@id/portraitRegister" />
        <action
            android:id="@+id/action_portraitLogin_to_portraitHome"
            app:destination="@id/portraitHome" />
    </fragment>
    <fragment
        android:id="@+id/portraitHome"
        android:name="com.example.mediavault.PortraitHome"
        android:label="fragment_portrait_home"
        tools:layout="@layout/fragment_portrait_home" >
        <action
            android:id="@+id/action_portraitHome_to_portraitAbout"
            app:destination="@id/portraitAbout" />
        <action
            android:id="@+id/action_portraitHome_to_portraitYourPlaylists"
            app:destination="@id/portraitYourPlaylists" />
        <action
            android:id="@+id/action_portraitHome_to_portraitYourAlbums"
            app:destination="@id/portraitYourAlbums" />
        <action
            android:id="@+id/action_portraitHome_to_portraitTags"
            app:destination="@id/portraitTags" />
    </fragment>
    <fragment
        android:id="@+id/portraitAbout"
        android:name="com.example.mediavault.PortraitAbout"
        android:label="fragment_portrait_about"
        tools:layout="@layout/fragment_portrait_about" >
        <action
            android:id="@+id/action_portraitAbout_to_portraitHome"
            app:destination="@id/portraitHome" />
        <action
            android:id="@+id/action_portraitAbout_to_portraitYourAlbums"
            app:destination="@id/portraitYourAlbums" />
        <action
            android:id="@+id/action_portraitAbout_to_portraitTags"
            app:destination="@id/portraitTags" />
        <action
            android:id="@+id/action_portraitAbout_to_portraitYourPlaylists"
            app:destination="@id/portraitYourPlaylists" />
    </fragment>
    <fragment
        android:id="@+id/portraitYourPlaylists"
        android:name="com.example.mediavault.PortraitYourPlaylists"
